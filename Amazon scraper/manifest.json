{
    "manifest_version": 2,
    "name": "Background Changer",
    "version": "1.0.0",
    "description": "Tracks prices of amazon products.",
    "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    },
    "browser_action": {
      "default_icon": "icon16.png",
      "default_popup": "popup.html"
    },
    "background":{
      "scripts":["jquery-3.6.0.js","background.js"]
  },
    "permissions": ["<all_urls>","tabs", "activeTab","webNavigation"],
    "content_security_policy":"script-src 'self' https://ajax.googleapis.com https://cdnjs.cloudflare.com; object-src 'self'"
  }